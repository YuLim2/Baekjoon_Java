package week01;

import java.util.Scanner;

public class Baekjoon_2588 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n1, n2;
		
		System.out.println("첫번째 세자리수 입력 : ");
		n1 = sc.nextInt();
		System.out.println("두번째 세자리수 입력 : ");
		n2 = sc.nextInt();
		
		System.out.println(n1 * (n2%10));
	}

}
